  "code_examples": {
    "python_basic": {
      "description": "Basic synchronous prediction",
      "code": "import replicate\n\noutput = replicate.run(\n    \"black-forest-labs/flux-1.1-pro\",\n    input={\n        \"prompt\": \"a photo of a cat\",\n        \"aspect_ratio\": \"1:1\"\n    }\n)\nprint(output)"
    },
    "python_async": {
      "description": "Async with webhook",
      "code": "import replicate\n\nprediction = replicate.predictions.create(\n    model=\"black-forest-labs/flux-1.1-pro\",\n    input={\"prompt\": \"a photo of a cat\"},\n    webhook=\"https://your-domain.com/webhook\",\n    webhook_events_filter=[\"completed\"]\n)\nprint(f\"Prediction ID: {prediction.id}\")"
    },
    "python_streaming": {
      "description": "Stream LLM output",
      "code": "import replicate\n\nfor event in replicate.stream(\n    \"meta/llama-3-70b-instruct\",\n    input={\"prompt\": \"Explain quantum computing\"}\n):\n    print(str(event), end=\"\")"
    },
    "curl_basic": {
      "description": "Create prediction via cURL",
      "code": "curl -s -X POST https://api.replicate.com/v1/predictions \\\n  -H \"Authorization: Bearer $REPLICATE_API_TOKEN\" \\\n  -H \"Content-Type: application/json\" \\\n  -d '{\n    \"version\": \"MODEL_VERSION_ID\",\n    \"input\": {\"prompt\": \"a photo of a cat\"}\n  }'"
    },
    "javascript_basic": {
      "description": "Node.js usage",
      "code": "import Replicate from 'replicate';\n\nconst replicate = new Replicate();\n\nconst output = await replicate.run(\n  \"black-forest-labs/flux-1.1-pro\",\n  { input: { prompt: \"a photo of a cat\" } }\n);\nconsole.log(output);"
    },
    "n8n_http_node": {
      "description": "n8n HTTP Request node config",
      "code": "{\n  \"url\": \"https://api.replicate.com/v1/predictions\",\n  \"method\": \"POST\",\n  \"authentication\": \"genericCredentialType\",\n  \"headers\": {\n    \"Content-Type\": \"application/json\"\n  },\n  \"body\": {\n    \"version\": \"{{MODEL_VERSION}}\",\n    \"input\": {\n      \"prompt\": \"{{$json.prompt}}\"\n    }\n  }\n}"
    }
