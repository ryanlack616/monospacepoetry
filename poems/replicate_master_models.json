{
  "meta": {
    "title": "Replicate Master Model Reference",
    "version": "2.0",
    "updated": "2026-02-06",
    "api_base": "https://api.replicate.com/v1",
    "docs": "https://replicate.com/docs"
  },
  "api": {
    "authentication": {
      "header": "Authorization",
      "format": "Bearer {REPLICATE_API_TOKEN}",
      "env_var": "REPLICATE_API_TOKEN"
    },
    "endpoints": {
      "predictions": {
        "create": "POST /predictions",
        "get": "GET /predictions/{id}",
        "cancel": "POST /predictions/{id}/cancel",
        "list": "GET /predictions"
      },
      "models": {
        "get": "GET /models/{owner}/{name}",
        "versions": "GET /models/{owner}/{name}/versions",
        "list": "GET /models"
      },
      "deployments": {
        "create": "POST /deployments",
        "get": "GET /deployments/{owner}/{name}",
        "predictions": "POST /deployments/{owner}/{name}/predictions"
      }
    },
    "webhooks": {
      "events": ["start", "output", "logs", "completed"],
      "payload_example": {
        "id": "prediction_id",
        "status": "succeeded",
        "output": "result_url_or_data"
      }
    },
    "rate_limits": {
      "requests_per_minute": 600,
      "concurrent_predictions": 10
    }
  },
